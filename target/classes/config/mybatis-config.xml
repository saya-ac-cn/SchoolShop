<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<!-- 打印查询语句 -->
		<setting name="logImpl" value="STDOUT_LOGGING" />
	</settings>

	<!-- 命名空间 -->
	<typeAliases>
		<typeAlias alias="Student" type="api.entity.StudentEntity"/>
		<typeAlias alias="Shop" type="api.entity.ShopEntity"/>
		<typeAlias alias="Files" type="api.entity.FilesEntity"/>
		<typeAlias alias="Stand" type="api.entity.StandEntity"/>
		<typeAlias alias="Goods" type="api.entity.GoodsEntity"/>
		<typeAlias alias="Order" type="api.entity.OrderEntity"/>
		<typeAlias alias="OrderDetail" type="api.entity.OrderDetailEntity"/>
		<typeAlias alias="User" type="api.entity.UserEntity"/>
		<typeAlias alias="OrderReport" type="api.entity.OrderReport"/>
		<typeAlias alias="Wallet" type="api.entity.WalletEntity"/>
		<typeAlias alias="Admin" type="api.entity.AdminEntity"/>
		<typeAlias alias="Message" type="api.entity.MessageEntity"/>
	</typeAliases>


</configuration>